  "name": "Nested Conditional Comment",
  "options": {},
  "html": "<!--[if gt IE 8]><!-->  <link rel=\"stylesheet\" media=\"screen, projection\" type=\"text/css\" href=\"http://www.website.com/_ui/desktop/assets/css/common.css\" /> <!--<![endif]-->",
