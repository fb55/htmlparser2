{
  "name": "htmlparser2",
  "description": "Fast & forgiving HTML/XML/RSS parser",
  "version": "3.6.0",
  "author": "Felix Boehm <me@feedic.com>",
  "keywords": [
    "html",
    "parser",
    "streams",
    "xml",
    "dom",
    "rss",
    "feed",
    "atom"
  ],
  "repository": {
    "type": "git",
    "url": "git://github.com/fb55/htmlparser2.git"
  },
  "bugs": {
    "mail": "me@feedic.com",
    "url": "http://github.com/fb55/htmlparser2/issues"
  },
  "directories": {
    "lib": "lib/"
  },
  "main": "lib/index.js",
  "scripts": {
    "test": "mocha && npm run lint",
    "lint": "jshint {lib,test}/*.js test/*/*.js"
  },
  "dependencies": {
    "domhandler": "2.2",
    "domutils": "1.3",
    "domelementtype": "1",
    "readable-stream": "1.1"
  },
  "devDependencies": {
    "mocha": "1",
    "jshint": "2"
  },
  "license": "MIT",
  "jshintConfig": {
    "eqeqeq": true,
    "freeze": true,
    "latedef": "nofunc",
    "noarg": true,
    "nonbsp": true,
    "quotmark": "double",
    "undef": true,
    "unused": true,
    "trailing": true,
    "eqnull": true,
    "proto": true,
    "smarttabs": true,
    "node": true,
    "globals": {
      "describe": true,
      "it": true
    }
  }
}
