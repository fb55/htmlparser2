{
    "name": "open-implies-close case of (non-br) void close tag in non-XML mode",
    "input": "<select><input><br></select>",
    "options": {
        "parser": {
            "lowerCaseAttributeNames": true
        }
    },
    "expected": [
        {
            "event": "opentagname",
            "startIndex": 0,
            "endIndex": 7,
            "data": ["select"]
        },
        {
            "event": "opentag",
            "startIndex": 0,
            "endIndex": 7,
            "data": ["select", {}, false]
        },
        {
            "event": "closetag",
            "startIndex": 8,
            "endIndex": 14,
            "data": ["select", true]
        },
        {
            "event": "opentagname",
            "startIndex": 8,
            "endIndex": 14,
            "data": ["input"]
        },
        {
            "event": "opentag",
            "startIndex": 8,
            "endIndex": 14,
            "data": ["input", {}, false]
        },
        {
            "event": "closetag",
            "startIndex": 8,
            "endIndex": 14,
            "data": ["input", true]
        },
        {
            "event": "opentagname",
            "startIndex": 15,
            "endIndex": 18,
            "data": ["br"]
        },
        {
            "event": "opentag",
            "startIndex": 15,
            "endIndex": 18,
            "data": ["br", {}, false]
        },
        {
            "event": "closetag",
            "startIndex": 15,
            "endIndex": 18,
            "data": ["br", true]
        }
    ]
}
