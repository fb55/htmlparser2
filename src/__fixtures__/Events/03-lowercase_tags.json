{
    "name": "Lowercase tags",
    "input": "<H1 class=test>adsf</H1>",
    "options": {
        "parser": {
            "lowerCaseTags": true
        }
    },
    "expected": [
        {
            "event": "opentagname",
            "data": ["h1"]
        },
        {
            "event": "attribute",
            "data": ["class", "test", null]
        },
        {
            "event": "opentag",
            "data": [
                "h1",
                {
                    "class": "test"
                }
            ]
        },
        {
            "event": "text",
            "data": ["adsf"]
        },
        {
            "event": "closetag",
            "data": ["h1"]
        }
    ]
}
